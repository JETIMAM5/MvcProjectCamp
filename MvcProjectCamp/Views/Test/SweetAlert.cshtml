
@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />
<script src="https://unpkg.com/sweetalert/dist/sweetalert.min.js"></script>
<script src="~/Scripts/jquery-3.4.1.js"></script>
<button class="btn btn-primary" id="btn1">Button 1</button>
<button class="btn btn-warning" id="btn2">Button 2</button>
<button class="btn btn-danger" id="btn3">Button 3</button>

<script>
    $('#btn1').click(function () {
        swal("Hello , MVC Project Camp Client");
    })
</script>
<script>
    $('#btn2').click(function () {
        swal("Here's the purpose of the project!", "Learn How MVC and N Layer Architecture works!");
    })
</script>
<script>
    $('#btn3').click(function () {
        swal({
            title: "Good job!",
            text: "You Done With This Project!",
            icon: "success",
        });

    })
</script>
